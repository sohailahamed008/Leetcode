class Solution:
    def sortColors(self, nums: List[int]) -> None:
        z=0
        o=0
        t=0
        for i in range(len(nums)):
            if nums[i]==0:
                z+=1
            elif nums[i]==1:
                o+=1
            else:
                t+=1
        for i in range(z):
            nums[i]=0
        for i in range(z,z+o):
            nums[i]=1
        for i in range(z+o,len(nums)):
            nums[i]=2
